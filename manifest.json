{
  "manifest_version": 2,
  "name": "DPlus Party - Disney Plus Watch Party",
  "description": "Watch Disney+ (Disney Plus) together anywhere, anytime with DPlus Party!",
  "version": "1.4.2",
  "icons": { "128": "images/dplus.png" },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["dplusparty.js", "dplus_chat.css", "jquery-3.4.1.min.js"]
}